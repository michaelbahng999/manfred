    "@types/lodash": "^4.14.70",
    "@types/moment": "^2.13.0",
    "@types/node": "^8.0.14",
    "@types/winston": "^2.3.3",
    "body-parser": "^1.17.2",
    "chrono-node": "^1.3.4",
    "discord.js": "^11.1.0",
    "express": "^4.15.3",
    "express-validator": "^3.2.1",
    "knex": "^0.13.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "pg": "^7.0.2",
    "sherlockjs": "^1.3.7",
    "ts-node": "^3.2.1",
    "typescript": "^2.4.2",
    "typestate": "^1.0.4",
    "winston": "^2.3.1"
